<ruleconfig>
    <entity pattern="*">
        <target name="breadcrumb">
            <rules>
                <rule type="regex" out_group="1"> <![CDATA[ ([a-z]+)\s*([0-9]+).* ]]> </rule>
                <rule type="css_select"> <![CDATA["ul breadcrumbs"]]> </rule>
                <rule type="javascript"> <![CDATA[some_javascript_here]]> </rule>
            </rules>
        </target>
        <target name="title">
            <rules>
                <rule type="css_select"> <![CDATA["ul breadcrumbs"]]> </rule>
                <rule type="regex" out_group="1"> <![CDATA[ ([a-z]+)\s*([0-9]+).* ]]> </rule>
            </rules>
        </target>
    </entity>
    <entity pattern="flipkart.com/*">
        <target name="title">
            <rules>
                <rule type="css_select"> <![CDATA["ul breadcrumbs"]]> </rule>
                <rule type="regex" out_group="1"> <![CDATA[ ([a-z]+)\s*([0-9]+).* ]]> </rule>
            </rules>
        </target>
    </entity>
</ruleconfig>