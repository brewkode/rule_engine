<ruleconfig>
    <entity pattern="*">
        <target name="breadcrumb">
            <rules>
                <rule type="css"> <![CDATA["ul breadcrumbs"]]> </rule>
                <rule type="regex" group_num="2"> <![CDATA[ ([a-z]+)\s*([0-9]+).* ]]> </rule>
            </rules>
        </target>
        <target name="title">
            <rules>
                <rule type="regex" group_num="1"> <![CDATA[ ([a-z]+)\s*([0-9]+).* ]]> </rule>
            </rules>
        </target>
    </entity>
    <entity pattern="flipkart.com/*">
        <target name="breadcrumb">
            <rules>
                <rule type="css"> <![CDATA["ul breadcrumbs"]]> </rule>
                <rule type="regex" group_num="1"> <![CDATA[ ([a-z]+)\s*([0-9]+).* ]]> </rule>
            </rules>
        </target>
    </entity>
</ruleconfig>