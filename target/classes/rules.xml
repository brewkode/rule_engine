<ruleconfig>
    <entity pattern="*">
        <target name="breadcrumb">
            <rules>
                <rule type="regex" out_group="1"> <![CDATA[ ([a-z]+)\s*([0-9]+).* ]]> </rule>
                <rule type="css_select"> <![CDATA["ul breadcrumbs"]]> </rule>
                <rule type="loop" start="2" end="4">
                    <rule type="regex" out_group="1"> <![CDATA[ ([a-z]+)\s*([0-9]+).* ]]> </rule>
                    <rule type="css_select"> <![CDATA["ul breadcrumbs"]]> </rule>
                    <rule type="concat" separator=","/>
                </rule>
            </rules>
        </target>
        <target name="title">
            <rules>
                <rule type="css_select"> <![CDATA["ul breadcrumbs"]]> </rule>
                <rule type="regex" out_group="1"> <![CDATA[ ([a-z]+)\s*([0-9]+).* ]]> </rule>
            </rules>
        </target>
    </entity>
    <entity pattern="*">
        <target name="title">
            <rules>
                <rule type="css_select"> <![CDATA["ul breadcrumbs"]]> </rule>
                <rule type="regex" out_group="1"> <![CDATA[ ([a-z]+)\s*([0-9]+).* ]]> </rule>
            </rules>
        </target>
    </entity>
</ruleconfig>